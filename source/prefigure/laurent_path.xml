<diagram dimensions="(250,250)" margins="5"
         xmlns="https://prefigure.org">
  <definition>size=3.5</definition>
  <definition>angle=120</definition>
  <definition>r1=1.5</definition>
  <definition>r2=3</definition>
  <definition>p1=r1*(cos(radians(angle)),sin(radians(angle)))</definition>
  <definition>p2=r2*(cos(radians(angle)),sin(radians(angle)))</definition>
  <definition>n=0.3*rotate(normalize(p1-p2),pi/2)</definition>
  <definition>l(t)=(1-t)*p1 + t*p2</definition>
  <coordinates bbox="(-size,-size,size,size)">
    <axes decorations="no" stroke="gray"/>
    <line endpoints="(p1,p2)" stroke="blue"/>
    <circle center="(0,0)" radius="r1" stroke="blue"/>
    <circle center="(0,0)" radius="r2" stroke="blue"/>
    <arc center="(0,0)" radius="r1" range="(-90, -40)"
         arrows="1" arrow-width="5"
         stroke="blue"/>
    <arc center="(0,0)" radius="r2" range="(0,60)"
         arrows="1" arrow-width="5"
         stroke="blue"/>
    <arc center="(0,0)" radius="r1+0.3" range="(80, 50)"
         arrows="1" arrow-width="5"
         stroke="blue"/>
    <arc center="(0,0)" radius="r1+0.3" range="(195,165)"
         arrows="1" arrow-width="5"
         stroke="blue"/>
    <arc center="(0,0)" radius="r2-0.3" range="(75,105)"
         arrows="1" arrow-width="5"
         stroke="blue"/>
    <arc center="(0,0)" radius="r2-0.3" range="(135,165)"
         arrows="1" arrow-width="5"
         stroke="blue"/>
    <line endpoints="(n+l(0.7),n+l(0.3))" stroke="blue"
          arrows="1" arrow-width="5"/>
    <line endpoints="(-n+l(0.3),-n+l(0.7))" stroke="blue"
          arrows="1" arrow-width="5"/>
    <label p="r1*(cos(radians(-40)),sin(radians(-40)))" alignment="se">
      <m>\gamma_1</m>
    </label>
    <label p="r2*(cos(radians(60)),sin(radians(60)))" alignment="ne">
      <m>\gamma_2</m>
    </label>
           


  </coordinates>
</diagram>
