<diagram dimensions="(250,250)" margins="5"
         xmlns="https://prefigure.org">
  <coordinates bbox="(-6,-3,6,9)">
    <definition>z1=(3,5)</definition>
    <definition>z2=(-4.5,2)</definition>
    <definition>s = z1+z2 </definition>
    <axes decorations="no"/>
    <path start="z1" stroke="#999" dash="6 6">
      <lineto point="z1"/>
      <rlineto point="z2"/>
      <rlineto point="-z1"/>
    </path>
    <group at="z1">
      <vector v="z1" thickness="2"/>
      <label anchor="z1" alignment="ne">
        <m>z_1</m>
      </label>
    </group>
    
    <group at="z2">
      <vector v="z2" thickness="2"/>
      <label anchor="z2" alignment="sw">
        <m>z_2</m>
      </label>
    </group>
    
    <group at="sum">
      <vector v="s" thickness="2"/>
      <label anchor="s" alignment="nw">
        <m>z_1+z_2</m>
      </label>
    </group>
    
  </coordinates>
</diagram>

      
