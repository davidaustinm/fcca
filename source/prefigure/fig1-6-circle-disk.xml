      <xlabel alignment="sw"><m>x</m></xlabel>
      <ylabel alignment="sw"><m>y</m></ylabel>
    </axes>
    <circle center="(2,1)" radius="2" stroke="black"/>
    <circle center="(-2,0)" radius="1/3" fill="black"/>
    <label anchor="(2.5,3)" alignment="ne">
      <m>C[2+i,2]</m>
    </label>
    <label anchor="(-2,-1/3)" alignment="south">
      <m>D[-2,\frac13]</m>
    </label>
  </coordinates>
