<diagram dimensions="(250,250)" margins="5"
         xmlns="https://prefigure.org">
  <definition>radius=0.8</definition>
  <coordinates bbox="(0,0,10,10)">
    <repeat parameter="p in ((6,2),(8,3),(4,4),(5.5,6),(2,6.5))">
      <point p="p"/>
      <circle center="p" radius="radius" stroke="blue"/>
      <definition>arrow_loc=p+radius*(1/sqrt(2),1/sqrt(2))</definition>
      <line endpoints="(arrow_loc, arrow_loc+(-0.1,0.1))" stroke="blue"
            arrows="1" arrow-width="5"/>
    </repeat>

    <repeat parameter="p in ((8.3,8),(3,9.5),(1.5,1))">
      <point p="p"/>
    </repeat>

    <spline points="((8,6),(6.5,7),(5,7.5),(3,8),(1.5,8),(0.5,7),(0.75,5),(2,3),(5,1),(7,0.5),(9,1),(9.5,2),(9.5,3),(9,4.5),(8,6))"
            stroke="blue" closed="yes"/>
    <spline points="((8,6),(6.5,7),(5,7.5),(3,8),(1.5,8),(0.5,7),(0.75,5),(2,3),(5,1),(7,0.5),(9,1),(9.5,2),(9.5,3),(9,4.5),(8,6))"
            arrows="1" domain="(0,2.5)" arrow-width="5"
            stroke="blue" closed="yes"/>

    <label anchor="(4,8)" alignment="ne">
      <m>\gamma</m>
    </label>

  </coordinates>
</diagram>
