<diagram dimensions="(250,250)" margins="5"
         xmlns="https://prefigure.org">
  <coordinates bbox="(-1,-3,5,3)">
    <axes decorations="no"/>
    <definition>z=(3.5,2)</definition>
    <definition>zbar=(3.5,-2)</definition>
    <vector v="z" thickness="2"/>
    <vector v="zbar" thickness="2"/>
    <label anchor="z" alignment="se">
      <m>z</m>
    </label>
    <label anchor="zbar" alignment="ne">
      <m>\overline{z}</m>
    </label>
  </coordinates>
</diagram>
