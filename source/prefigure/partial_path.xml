<diagram dimensions="(250,250)" margins="5"
         xmlns="https://prefigure.org">
  <definition>size=2.5</definition>
  <definition>a1=105</definition>
  <definition>a2=-50</definition>
  <definition>r=2</definition>
  <definition>p1=r*(cos(radians(a1)),sin(radians(a1)))</definition>
  <definition>p2=r*(cos(radians(a2)),sin(radians(a2)))</definition>
  <definition>l(t)=(1-t)*p1 + t*p2</definition>
  <coordinates bbox="(-size,-size,size,size)">
    <axes decorations="no" stroke="gray"/>
    <line endpoints="(p1,p2)" stroke="blue"/>
    <circle center="(0,0)" radius="r" stroke="blue"/>

    <arc center="(0,0)" radius="r" range="(a2, 40)"
         arrows="1" arrow-width="5"
         stroke="blue"/>

    <arc center="(0,0)" radius="r" range="(a1, 220)"
         arrows="1" arrow-width="5"
         stroke="blue"/>

    <line endpoints="(p1, l(0.4))" stroke="blue"
          arrows="1" arrow-width="5"/>
    <line endpoints="(p2, l(0.6))" stroke="blue"
          arrows="1" arrow-width="5"/>

    <point p="(0,0)" alignment="nw"><m>0</m></point>
    <point p="(1,0)"><m>1</m></point>
    <label p="2*(cos(radians(220)),sin(radians(220)))" alignment="sw">
      <m>\gamma_1</m>
    </label>
    <label p="2*(cos(radians(40)),sin(radians(40)))" alignment="ne">
      <m>\gamma_2</m>
    </label>
           


  </coordinates>
</diagram>
