  <exercise xml:id="ex_1_0">
    <introduction>
      <p>
        Let <m>z = 1+2i</m> and <m>w = 2-i</m>.
        Compute the following:
      </p>
    </introduction>
    <task>
      <statement>
          <p>
            <m>z + 3w</m>
          </p>
      </statement>
      <answer>
        <p>
          <m>7 - i</m>
        </p>
      </answer>
    </task>
    <task>
      <statement>
        <p>
          <m>\overline{w} - z</m>
        </p>
      </statement>
      <answer>
        <p>
          <m>1 - 3i</m>
        </p>
      </answer>
    </task>
    <task>
      <statement>
        <p>
          <m>z^3</m>
        </p>
      </statement>
      <answer>
        <p>
          <m>-11 - 2i</m>
        </p>
      </answer>
    </task>
    <task>
      <statement>
        <p>
          <m>\Re(w^2 + w)</m>
        </p>
      </statement>
      <answer>
        <p>
          <m>5</m>
        </p>
      </answer>
    </task>
    <task>
      <statement>
        <p>
          <m>z^2 + \overline{z} + i</m>
        </p>
      </statement>
      <answer>
        <p>
          <m>-2 + 7i</m>
        </p>
      </answer>
    </task>
  </exercise>
  <exercise xml:id="ex_1_1">
    <introduction>
      <p>
        Find the real and imaginary parts of each of the following:
      </p>
    </introduction>
    <task>
      <statement>
        <p>
          <m>\frac{z - a}{z + a}</m> for any <m>a \in \R</m>
        </p>
      </statement>
    </task>
    <task>
      <statement>
        <p>
          <m>\frac{3 + 5i}{7i + 1}</m>
        </p>
      </statement>
      <answer>
        <p>
          <m>\frac {19} {25} - \frac {8} {25} i</m>
        </p>
      </answer>
    </task>
    <task>
      <statement>
        <p>
          <m>\left( \frac{-1 + i\sqrt{3}}{2} \right)^3</m>
        </p>
      </statement>
      <answer>
        <p>
          <m>-1</m>
        </p>
      </answer>
    </task>
    <task>
      <statement>
        <p>
          <m>i^n</m> for any <m>n \in \Z</m>
        </p>
      </statement>
    </task>
  </exercise>
